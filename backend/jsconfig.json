{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#root/*": [
        "./*"
      ],
      "#utils/*": [
        "src/utils/*"
      ],
      "#configs/*": [
        "src/configs/*"
      ],
      "#handlers/*": [
        "src/handlers/*"
      ],
      "#entities/*": [
        "src/entities/*"
      ],
      "#models/*": [
        "src/models/*"
      ],
      "#plugins/*": [
        "src/plugins/*"
      ],
      "#routes/*": [
        "src/routes/*"
      ],
      "#schemas/*": [
        "src/schemas/*"
      ],
      "#middlewares/*": [
        "src/middlewares/*"
      ],
      "#lib/*": [
        "src/lib/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}