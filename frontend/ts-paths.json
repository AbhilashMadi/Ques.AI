{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@css/*": [
        "./src/css/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@configs/*": [
        "./src/configs/*"
      ],
      "@custom": [
        "./src/components/custom/index.ts",
      ],
      "@icons": [
        "./src/components/icons.tsx",
      ],
      "@assets": [
        "./src/assets/index.ts",
      ],
      "@schemas/*": [
        "./src/schemas/*",
      ],
      "@resources/*": [
        "./src/resources/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
    }
  }
}